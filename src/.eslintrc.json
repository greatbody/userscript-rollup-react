{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["airbnb-base", "plugin:react/recommended"],
  "rules": {
    "semi": 2,
    "no-undef": 0,
    "no-param-reassign": 0,
    "no-eval": 0,
    "no-console": 0,
    "no-alert": 0
  },
  "plugins": [
    "react"
  ]
}
